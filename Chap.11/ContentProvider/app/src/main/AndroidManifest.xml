<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.techtown.contentprovider">
    <permission android:name="org.techtown.contentprovider.READ_DATABASE" android:protectionLevel="normal"/>
    <permission android:name="org.techtown.contentprovider.WRITE_DATABASE" android:protectionLevel="normal"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ContentProvider">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <provider
            android:authorities="org.techtown.contentprovider"
            android:name=".StudentProvider"
            android:exported="true"
            android:readPermission="org.techtown.contentprovider.READ_DATABASE"
            android:writePermission="org.techtown.contentprovider.WRITE_DATABASE"/>
    </application>

</manifest>